<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
		Width="800" Height="600" WindowStartupLocation="CenterScreen"
        x:Class="Argon.Editor.Views.ModuleWindow"
        Title="Argon Editor">
	<DockPanel Margin="10">
		<TextBlock DockPanel.Dock="Right" Width="200">Test 4</TextBlock>
		<StackPanel Margin="5" DockPanel.Dock="Top" Orientation="Horizontal">
			<Button Margin="5">Create new module</Button>
			<Button Margin="5">Add existing module</Button>
		</StackPanel>

		<StackPanel Margin="5" DockPanel.Dock="Top" Orientation="Vertical">
			<Border Background="DimGray" BorderThickness="1" Padding="5" Margin="5">
				<TextBlock>Test 1</TextBlock>
			</Border>
			<Border Background="DimGray" BorderThickness="1" Padding="5" Margin="5">
				<TextBlock>Test 2</TextBlock>
			</Border>
			<Border Background="DimGray" BorderThickness="1" Padding="5" Margin="5">
				<TextBlock>Test 3</TextBlock>
			</Border>
			<Border Background="DimGray" BorderThickness="1" Padding="5" Margin="5">
				<TextBlock>Test 4</TextBlock>
			</Border>
		</StackPanel>
	</DockPanel>
</Window>
